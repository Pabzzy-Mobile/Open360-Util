{
  "name": "open360-util",
  "description": "Util module of Open360",
  "version": "0.1.2",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "husky-this-is-just-a-tag-to-tag-this-to-not-be-used": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "author": "Pedro Pires",
  "module": "dist/open360-util.esm.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Pabzzy-Mobile/Open360-Util.git"
  },
  "keywords": [
    "Node.js",
    "Utility"
  ],
  "bugs": {
    "url": "https://github.com/Pabzzy-Mobile/Open360-Util/issues"
  },
  "homepage": "https://github.com/Pabzzy-Mobile/Open360-Util#readme",
  "size-limit": [
    {
      "path": "dist/open360-util.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/open360-util.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^4.9.0",
    "husky": "^4.3.0",
    "size-limit": "^4.9.0",
    "tsdx": "^0.14.1",
    "tslib": "^2.0.3",
    "typescript": "^4.1.2"
  },
  "dependencies": {
    "@types/express": "^4.17.9",
    "@types/passport": "^1.0.4",
    "@types/socket.io": "^2.1.11",
    "express": "^4.17.1",
    "passport": "^0.4.1",
    "socket.io": "^3.0.3"
  }
}
